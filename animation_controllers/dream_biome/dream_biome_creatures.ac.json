{
    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.dream_biome.ground_passive": {
            "initial_state": "idle",
            "states": {
                "idle": {
                    "animations": ["anim_idle"],
                    "transitions": [
                        { "walking": "query.modified_move_speed > 0.05" }
                    ],
                    "blend_transition": 0.2
                },
                "walking": {
                    "animations": ["anim_walk"],
                    "transitions": [
                        { "idle": "query.modified_move_speed < 0.02" }
                    ],
                    "blend_transition": 0.2
                }
            }
        },
        "controller.animation.dream_biome.ground_passive_runner": {
            "initial_state": "idle",
            "states": {
                "idle": {
                    "animations": ["anim_idle"],
                    "transitions": [
                        { "running": "query.is_moving && query.modified_move_speed >= 0.4" },
                        { "walking": "query.is_moving" }
                    ],
                    "blend_transition": 0.2
                },
                "walking": {
                    "animations": ["anim_walk"],
                    "transitions": [
                        { "running": "query.modified_move_speed >= 0.4" },
                        { "idle": "!query.is_moving" }
                    ],
                    "blend_transition": 0.2
                },
                "running": {
                    "animations": ["anim_run"],
                    "transitions": [
                        { "walking": "query.is_moving && query.modified_move_speed < 0.4" },
                        { "idle": "!query.is_moving" }
                    ],
                    "blend_transition": 0.2
                }
            }
        },
        "controller.animation.dream_biome.ground_hostile": {
            "initial_state": "idle",
            "states": {
                "idle": {
                    "animations": ["anim_idle"],
                    "transitions": [
                        { "attacking": "query.is_delayed_attacking" },
                        { "walking": "query.is_moving" }
                    ],
                    "blend_transition": 0.2
                },
                "walking": {
                    "animations": ["anim_walk"],
                    "transitions": [
                        { "attacking": "query.is_delayed_attacking" },
                        { "idle": "!query.is_moving" }
                    ],
                    "blend_transition": 0.2
                },
                "attacking": {
                    "animations": ["anim_attack"],
                    "transitions": [
                        { "idle": "!query.is_delayed_attacking || query.all_animations_finished" }
                    ],
                    "blend_transition": 0.15
                }
            }
        },
        "controller.animation.dream_biome.ground_neutral": {
            "initial_state": "idle",
            "states": {
                "idle": {
                    "animations": ["anim_idle"],
                    "transitions": [
                        { "attacking": "query.is_delayed_attacking" },
                        { "running": "query.is_moving && query.modified_move_speed >= 0.4" },
                        { "walking": "query.is_moving" }
                    ],
                    "blend_transition": 0.2
                },
                "walking": {
                    "animations": ["anim_walk"],
                    "transitions": [
                        { "attacking": "query.is_delayed_attacking" },
                        { "running": "query.modified_move_speed >= 0.4" },
                        { "idle": "!query.is_moving" }
                    ],
                    "blend_transition": 0.2
                },
                "running": {
                    "animations": ["anim_run"],
                    "transitions": [
                        { "attacking": "query.is_delayed_attacking" },
                        { "walking": "query.is_moving && query.modified_move_speed < 0.4" },
                        { "idle": "!query.is_moving" }
                    ],
                    "blend_transition": 0.2
                },
                "attacking": {
                    "animations": ["anim_attack"],
                    "transitions": [
                        { "idle": "!query.is_delayed_attacking || query.all_animations_finished" }
                    ],
                    "blend_transition": 0.15
                }
            }
        },
        "controller.animation.dream_biome.flyer_passive": {
            "initial_state": "idle",
            "states": {
                "idle": {
                    "animations": ["anim_idle"],
                    "transitions": [
                        { "flying": "query.modified_move_speed > 0.1" }
                    ],
                    "blend_transition": 0.25
                },
                "flying": {
                    "animations": ["anim_fly"],
                    "transitions": [
                        { "idle": "query.modified_move_speed < 0.05" }
                    ],
                    "blend_transition": 0.25
                }
            }
        },
        "controller.animation.dream_biome.flyer_hostile": {
            "initial_state": "idle",
            "states": {
                "idle": {
                    "animations": ["anim_idle"],
                    "transitions": [
                        { "attacking": "variable.attack_time > 0" },
                        { "flying": "!query.is_on_ground || query.modified_move_speed > 0.4" }
                    ],
                    "blend_transition": 0.3
                },
                "flying": {
                    "animations": ["anim_fly"],
                    "transitions": [
                        { "attacking": "variable.attack_time > 0" },
                        { "idle": "query.is_on_ground && query.modified_move_speed < 0.1" }
                    ],
                    "blend_transition": 0.3
                },
                "attacking": {
                    "animations": ["anim_attack"],
                    "transitions": [
                        { "flying": "variable.attack_time <= 0 && (!query.is_on_ground || query.modified_move_speed > 0.4)" },
                        { "idle": "variable.attack_time <= 0" }
                    ],
                    "blend_transition": 0.2
                }
            }
        },
        "controller.animation.dream_biome.aquatic": {
            "initial_state": "idle",
            "states": {
                "idle": {
                    "animations": ["anim_idle"],
                    "transitions": [
                        { "swimming": "query.modified_move_speed > 0.05" }
                    ],
                    "blend_transition": 0.2
                },
                "swimming": {
                    "animations": ["anim_walk"],
                    "transitions": [
                        { "idle": "query.modified_move_speed < 0.02" }
                    ],
                    "blend_transition": 0.2
                }
            }
        },
        "controller.animation.dream_biome.moonlit_wolf": {
            "initial_state": "idle",
            "states": {
                "idle": {
                    "animations": ["anim_idle"],
                    "transitions": [
                        { "attacking": "query.is_delayed_attacking" },
                        { "walking": "query.is_moving" }
                    ],
                    "blend_transition": 0.2
                },
                "walking": {
                    "animations": ["anim_walk"],
                    "transitions": [
                        { "attacking": "query.is_delayed_attacking" },
                        { "running": "query.modified_move_speed >= 0.33" },
                        { "idle": "!query.is_moving" }
                    ],
                    "blend_transition": 0.2
                },
                "running": {
                    "animations": ["anim_run"],
                    "transitions": [
                        { "attacking": "query.is_delayed_attacking" },
                        { "walking": "query.is_moving && query.modified_move_speed < 0.33" },
                        { "idle": "!query.is_moving" }
                    ],
                    "blend_transition": 0.2
                },
                "attacking": {
                    "animations": ["anim_attack"],
                    "transitions": [
                        { "idle": "!query.is_delayed_attacking || query.all_animations_finished" }
                    ],
                    "blend_transition": 0.15
                }
            }
        },
        "controller.animation.dream_biome.shadow_child": {
            "initial_state": "idle",
            "states": {
                "idle": {
                    "animations": ["anim_idle"],
                    "transitions": [
                        { "skill": "query.is_delayed_attacking" },
                        { "walking": "query.is_moving" }
                    ],
                    "blend_transition": 0.2
                },
                "walking": {
                    "animations": ["anim_walk"],
                    "transitions": [
                        { "skill": "query.is_delayed_attacking" },
                        { "idle": "!query.is_moving" }
                    ],
                    "blend_transition": 0.2
                },
                "skill": {
                    "animations": ["anim_skill"],
                    "transitions": [
                        { "idle": "!query.is_delayed_attacking || query.all_animations_finished" }
                    ],
                    "blend_transition": 0.15
                }
            }
        }
    }
}
